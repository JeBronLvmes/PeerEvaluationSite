<div class="container-fluid bg-primary">
</div>
<p id="notice"><%= notice %></p>

<h1>Courses Student</h1>

<table class="table">
  <thead>
  <tr>
    <th>id</th>
    <th>course_id</th>
    <th>student_id</th>
  </tr>
  </thead>

  <tbody>
  <% @courses_students.each do |courses_student| %>
    <tr>
      <td><%= courses_student.id %></td>
      <td><%= courses_student.course_id %></td>
      <td><%= courses_student.student_id %></td>
      <td><%= link_to 'Destroy', courses_student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>